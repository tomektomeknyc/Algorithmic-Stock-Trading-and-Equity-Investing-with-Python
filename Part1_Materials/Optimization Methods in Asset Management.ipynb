    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Define risk-free rate and risk tolerance (variance constraint)\n",
    "risk_free_rate = 0.01  # Example risk-free rate (as a decimal)\n",
    "risk_tolerance = 0.000040\n",
    "\n",
    "# Define the expected return and variance of the risky portfolio (e.g., your original portfolio)\n",
    "expected_return_risk = 0.06  # Example expected return (as a decimal)\n",
    "variance_risk = 0.00025  # Example variance\n",
    "\n",
    "# Define the range of  values\n",
    "_values = np.linspace(-2, 2, 100)\n",
    "\n",
    "# Initialize lists to store portfolio statistics\n",
    "portfolio_returns = []\n",
    "portfolio_variances = []\n",
    "\n",
    "# Calculate portfolio statistics for different  values\n",
    "for  in _values:\n",
    "    portfolio_return =  * risk_free_rate + (1 - ) * expected_return_risk\n",
    "    portfolio_variance = ( ** 2) * 0 + ((1 - ) ** 2) * variance_risk + 2 *  * (1 - ) * 0\n",
    "    portfolio_returns.append(portfolio_return)\n",
    "    portfolio_variances.append(portfolio_variance)\n",
    "\n",
    "# Plot the efficient frontier\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.scatter(portfolio_variances, portfolio_returns, c=_values, cmap='viridis')\n",
    "plt.xlabel('Portfolio Variance')\n",
    "plt.ylabel('Portfolio Expected Return')\n",
    "plt.title('Efficient Frontier')\n",
    "plt.colorbar(label=' Values')\n",
    "plt.grid(True)\n",
    "plt.show()\n",
    "\n",
    "# Find the portfolio that meets the risk tolerance constraint\n",
    "optimal_return = None\n",
    "for i, variance in enumerate(portfolio_variances):\n",
    "    if variance <= risk_tolerance:\n",
    "        optimal_return = portfolio_returns[i]\n",
    "        break\n",
    "\n",
    "print(f\"Approximate Optimal Return for Risk Tolerance: {optimal_return:.8%}\")\n"
   ]
